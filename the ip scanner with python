import socket

def scan_ip(ip_address)
    try:
	socket.int_aton(ip_address)
	return True 
    except socket.error:
	return False

def main():
    start_ip = input("enter the starting ip addr: ")
    end_ip = input("enter the ending ip addr: ")

    for ip_addr in range(int(start_ip). int(end_ip) + 1)
	if scan_ip(str(ip_address)):
	    print(ip_address)

if __name__ == "__main__":
    main()
